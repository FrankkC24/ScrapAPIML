{
  "name": "spotify-scrap",
  "version": "1.4.2",
  "description": "Spotify Scrap",
  "main": "index.js",
  "scripts": {
    "setup": "npm install pnpm && pnpm install && pnpm run build",
    "build": "rimraf dist && tsc && pnpm run copy-json",
    "copy-json": "copyfiles -u 1 src/accounts/accounts.json dist/",
    "start": "node --max-old-space-size=4096 --expose-gc dist/server.js"
  },
  "keywords": [],
  "author": "FrankkC",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "puppeteer": "^21.5.2",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5"
  }
}
